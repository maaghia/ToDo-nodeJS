<h1>ToDo App</h1>
<h3>My List</h3>

<ul>
    <% todos.forEach(function(todo) { %>
        <li><%= todo %></li>
    <% }) %>
    
</ul>

<form name="todo" method="post" action="/add">
    <input type="text" name="todo" placeholder="what's up?"/><br><br>
    <input type="submit" value="accept"/><br><br><hr>
</form>


